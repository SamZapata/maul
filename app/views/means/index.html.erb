<p id="notice"><%= notice %></p>

<h1>Means</h1>

<table>
  <thead>
    <tr>
      <th>Nombrerecurso</th>
      <th>Tipo</th>
      <th>Cantidad</th>
      <th>Disponibilidad</th>
      <th>Observacion</th>
      <th>Aportado</th>
      <th>Pendiente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @means.each do |mean| %>
      <tr>
        <td><%= mean.nombreRecurso %></td>
        <td><%= mean.tipo %></td>
        <td><%= mean.Cantidad %></td>
        <td><%= mean.disponibilidad %></td>
        <td><%= mean.observacion %></td>
        <td><%= mean.aportado %></td>
        <td><%= mean.pendiente %></td>
        <td><%= link_to 'Show', mean %></td>
        <td><%= link_to 'Edit', edit_mean_path(mean) %></td>
        <td><%= link_to 'Destroy', mean, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mean', new_mean_path %>
